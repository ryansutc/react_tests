{
  "name": "arcgistest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@esri/react-arcgis": "^4.0.0",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "esri-loader": "^2.10.0",
    "express": "^4.17.1",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.9.0",
    "react-number-format": "^4.2.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "set EXTEND_ESLINT=true &&react-scripts start",
    "debug": "react-scripts --inspect=0.0.0.0 start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": ["react-app", "shared-config"],
    "rules": {
      "complexity": ["warn", { "max": 2 }],
      "quotes": ["error", "double"],
      "indent": ["error", 4]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}